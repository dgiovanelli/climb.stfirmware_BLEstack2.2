        "
        compilerBuildOptions="
        --cmd_file=${PROJECT_IMPORT_LOC}/../../iar/stack/build_config.opt
        --cmd_file=${TI_BLE_SDK_BASE}/src/config/build_components.opt
        -O4
        --opt_for_speed=0
        -mv7M3
        --code_state=16
        --abi=eabi
        -me
        --display_error_number
        --diag_wrap=off
        --diag_suppress=48
        --diag_suppress=16004
        --diag_warning=225
        --diag_warning=2
        --c99

        -DUSE_ICALL
        -DFLASH_ROM_BUILD
        -DPOWER_SAVING
        -DINCLUDE_AES_DECRYPT		
        -DOSAL_SNV=1
        -DxPM_DISABLE_PWRDOWN
        -DxTESTMODES		
        -DxTESTBLEBOARD
        -DOSAL_CBTIMER_NUM_TASKS=1
        -DxDEBUG
        -DxDEBUG_GPIO
        -DxDEBUG_ENC
        -DxDEBUG_SW_TRACE
		-DEXT_HAL_ASSERT
        -DNEAR_FUNC=
        -DDATA=
        -DCC26XXWARE
        -DCC26XX
        -Dccs

        -I${CG_TOOL_ROOT}/include        
		-I${SRC_BLE_CORE}/inc
        -I${SRC_BLE_CORE}/controller/cc26xx/inc
        -I${SRC_EX}/common/cc26xx		
        -I${SRC_EX}/common/cc26xx/npi/stack		
